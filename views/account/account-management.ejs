

<main>
  <h1><%= title %></h1>
  <% if (messages && messages.notice) { %>
    <div class="flash-message flash-success"><%= messages.notice %></div>
  <% } %>
  <% if (errors && errors.length > 0) { %>
    <ul class="errors">
      <% errors.forEach(function(error) { %>
        <li><%= error.msg %></li>
      <% }) %>
    </ul>
  <% } %>
  <p>You're logged in.</p>
</main>



